
<Container>:
    orientation: 'vertical'
    spacing: 50
    text_label: label
    button_label: butt
    Label:
        id:label
        text:' '
        pos_hint:{'y': 0.9, 'center_x': 0.5}
        font_size: 20
    Button:
        id:butt
        pos_hint:{'y': 0.4, 'center_x': 0.5}
        size_hint:0.2, 0.2
        text: 'Start'
        on_release:
            root.change_text()
